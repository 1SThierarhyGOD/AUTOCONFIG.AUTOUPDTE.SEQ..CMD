<Project>

  <PropertyGroup>
    <AspireHostingVersion>@VERSION@</AspireHostingVersion>
    <!-- this should be shared with Aspire.Hosting.Runtime.sfxproj -->
    <AspireHostingRuntimePackRids>win-x86;win-x64;win-arm64;linux-x64;linux-arm64;osx-x64;osx-arm64</AspireHostingRuntimePackRids>
  </PropertyGroup>

  <!-- in future this should be in the installer with all the other past and present known KnownFrameworkReferences -->
  <ItemGroup>
    <KnownFrameworkReference
      Include="Aspire.Hosting"
      TargetFramework="net8.0"
      RuntimeFrameworkName="Aspire.Hosting"
      DefaultRuntimeFrameworkVersion="8.0.0"
      LatestRuntimeFrameworkVersion="$(AspireHostingVersion)"
      TargetingPackName="Aspire.Hosting.Ref"
      TargetingPackVersion="$(AspireHostingVersion)"
      RuntimePackNamePatterns="Aspire.Hosting.Runtime.**RID**"
      RuntimePackRuntimeIdentifiers="@(AspireHostingRuntimePackRids, '%3B')"
      />
  </ItemGroup>

</Project>
